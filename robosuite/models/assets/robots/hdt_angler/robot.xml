<mujoco model="hdt">
	<compiler angle="radian" coordinate="local"></compiler>

	<option timestep="2e-3"
			iterations="10"
			tolerance="1e-10"
			impratio="10"
			gravity="0 0 -9.81"
			solver="PGS"
			cone="elliptic" />
	<size nuserdata="100" njmax="500" nconmax="1000" nstack="600000" nuser_body="9" nuser_jnt="1" nuser_site="1" nuser_tendon="1" nuser_actuator="12" nuser_sensor="1" />

	<!-- <visual>
		<map fogstart="3" fogend="5" force="0.1" znear="0.1"/>
		<quality shadowsize="2048" offsamples="8"/>
		<global offwidth="800" offheight="800"/>
    </visual> 
         -->
    <actuator>
      <velocity ctrllimited="true" ctrlrange="-2.0000 2.0000" joint="joint1" kv="1"/>
      <velocity ctrllimited="true" ctrlrange="-1.5708 1.5708" joint="joint2" kv="1"/>
      <velocity ctrllimited="true" ctrlrange="-3.0708 1.2708" joint="joint3" kv="1"/>
      <velocity ctrllimited="true" ctrlrange="-2.5708 2.5708" joint="joint4" kv="1"/>
      <velocity ctrllimited="true" ctrlrange="-1.5708 1.5708" joint="joint5" kv="1"/>
	  <velocity ctrllimited="true" ctrlrange="-1.5708 2.5708" joint="joint6" kv="1"/>
    </actuator>

    <asset>
        <mesh name="H026994 - A35 M-2 PEDESTAL ASSY - SHAREABLE" file="meshes/H026994 - A35 M-2 PEDESTAL ASSY - SHAREABLE.STL" />
