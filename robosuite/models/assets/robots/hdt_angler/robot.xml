        <mesh name="H024847 - A35 M-2 PROXIMAL TUBE ASSY - SHAREABLE" file="meshes/H024847 - A35 M-2 PROXIMAL TUBE ASSY - SHAREABLE.STL" />
        <mesh name="H024848 - A35 M-2 DISTAL TUBE ASSY - SHAREABLE" file="meshes/H024848 - A35 M-2 DISTAL TUBE ASSY - SHAREABLE.STL" />
        <mesh name="H024842 - A35 M-2 PINCER ASSY - SHAREABLE, UNDERWATER" file="meshes/H024842 - A35 M-2 PINCER ASSY - SHAREABLE, UNDERWATER.STL" />
